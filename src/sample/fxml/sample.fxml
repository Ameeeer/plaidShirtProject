<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="sample.Controllers.SignUpController">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <children>
        <Pane prefHeight="380.0" prefWidth="365.0">
            <children>
                <TextField fx:id="log" layoutX="58.0" layoutY="186.0" prefHeight="27.0" prefWidth="249.0"
                           promptText="Login"/>

                <PasswordField id="pass" fx:id="pass" layoutX="58.0" layoutY="233.0" prefHeight="27.0" prefWidth="249.0"
                               promptText="Password"/>
                <Button fx:id="signIn" layoutX="58.0" layoutY="268.0" mnemonicParsing="false" onAction="#setSignIn"
                        text="SignIn"/>
                <Button fx:id="registrate" layoutX="215.0" layoutY="268.0" mnemonicParsing="false" onAction="#setSignIn"
                        text="Registration"/>
                <ImageView fitHeight="115.0" fitWidth="171.0" layoutX="125.0" layoutY="26.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../../../kubik.png"/>
                    </image>
                </ImageView>
                <Text fx:id="textIfNotCorrected" layoutX="83.0" layoutY="332.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      wrappingWidth="199.21875"/>
            </children>
        </Pane>
    </children>
</GridPane>
